# Projeto-de-l-gica-comp.
função { inicio
  {
    escreva("digite um numero inteiro: n/ ")
    leia(número)
   {
    se (numero > 0) //verificar se numero e positivo
    }
    escreva (número positivo)
    {
    senao (numero < 0) //verificar se numero e negativo
    }
    escreva (numero negativo)
    {
    então escreva (n/ + 2)
    }
  }
